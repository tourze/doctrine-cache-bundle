{
  "name": "tourze/doctrine-cache-bundle",
  "description": "Doctrine Cache Bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/cache-contracts": "^3",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/doctrine-helper": "0.0.*",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineCacheBundle\\": "src"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}